{
  "functions": {
    "src/pages/**/*.astro": {
      "runtime": "@vercel/static"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/auth",
      "has": [
        {
          "type": "host",
          "value": "(?<host>.*\\.vercel\\.app)$"
        }
      ]
    }
  ]
}
